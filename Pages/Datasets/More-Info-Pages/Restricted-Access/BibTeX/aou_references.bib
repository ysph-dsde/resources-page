@misc{survey_explorer,
   abstract = {Participants in the All of Us Research Program respond to surveys spanning a variety of topics. The program has tested each survey for readability and accessibility using cognitive interviews and quantitative testing. This testing process included people from different educational backgrounds and geographic locations to capture a sample that reflects the U.S. population. After participants complete the core surveys (The Basics, Lifestyle, and Overall Health), they may complete additional surveys on other topics.

In addition to the source material below, more detailed information is available in the Survey Data Codebooks.},
   author = {All of Us},
   journal = {National Institutes of Health},
   title = {Survey Explorer},
   url = {https://www.researchallofus.org/data-tools/survey-explorer/}
}
